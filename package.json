{
  "name": "angular-express-starter",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ts-node ./server/index.ts",
    "dev": "ng serve --proxy-config proxy.conf.json",
    "dev-server": "nodemon -e ts -w ./server -x npm run server",
    "server": "ts-node ./server/app.ts",
    "build": "rimraf dist && ng build --prod --named-chunks && npm run precache",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "precache": "sw-precache --verbose --config=sw-precache-config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.3.6",
    "@angular/common": "^4.3.6",
    "@angular/compiler": "^4.3.6",
    "@angular/core": "^4.3.6",
    "@angular/forms": "^4.3.6",
    "@angular/http": "^4.3.6",
    "@angular/platform-browser": "^4.3.6",
    "@angular/platform-browser-dynamic": "^4.3.6",
    "@angular/platform-server": "^4.4.0-RC.0",
    "@angular/router": "^4.3.6",
    "@types/morgan": "^1.7.32",
    "body-parser": "^1.17.2",
    "core-js": "^2.4.1",
    "express": "^4.15.4",
    "method-override": "^2.3.9",
    "morgan": "^1.8.2",
    "rimraf": "^2.6.1",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.17",
    "@angular/cli": "1.3.0",
    "@angular/compiler-cli": "^4.3.6",
    "@angular/language-service": "^4.3.6",
    "@types/body-parser": "^1.16.5",
    "@types/express": "^4.0.37",
    "@types/jasmine": "^2.5.54",
    "@types/jasminewd2": "~2.0.2",
    "@types/method-override": "0.0.29",
    "@types/node": "^6.0.88",
    "codelyzer": "~3.1.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "sw-precache": "^5.2.0",
    "ts-node": "^3.3.0",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  },
  "devDependencies": {
    "nodemon": "^1.12.1"
  }
}

{
  "name": "angular-express-starter",
  "version": "0.2.0",
  "license": "MIT",
  "scripts": {
    "start": "ts-node --project server ./server/index.ts",
    "start:dev": "nodemon -e ts -w ./server -x npm run server",
    "dev": "ng serve --proxy-config proxy.conf.json",
    "server": "cross-env NODE_ENV=dev ts-node --project server ./server/index.ts",
    "deploy": "git push origin master && git push heroku master",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ng build --prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.1",
    "@angular/cli": "6.1.2",
    "@angular/common": "6.1.1",

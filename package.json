{
  "name": "angular-express-starter",
  "version": "0.2.0",
  "license": "MIT",
  "scripts": {
    "start": "npm run build:ssr && npm run server",
    "dev": "ng serve --proxy-config proxy.conf.json",
    "server": "nodemon dist/server.js",
    "build": "rimraf dist && ng build --prod",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "build:client-and-server-bundles": "ng build --prod && ng build --prod --app 1 --output-hashing=false",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.1",
    "@angular/cli": "6.1.2",
    "@angular/common": "6.1.1",
    "@angular/compiler": "6.1.1",
    "@angular/compiler-cli": "6.1.1",
    "@angular/core": "6.1.1",
    "@angular/forms": "6.1.1",
    "@angular/http": "6.1.1",
    "@angular/language-service": "6.1.1",
    "@angular/platform-browser": "6.1.1",
    "@angular/platform-browser-dynamic": "6.1.1",
    "@angular/platform-server": "6.1.1",
    "@angular/router": "6.1.1",
    "@nguniversal/module-map-ngfactory-loader": "5.0.0-beta.5",
    "@types/body-parser": "^1.16.5",
    "@types/express": "^4.0.37",
    "@types/jasmine": "^2.5.54",
    "@types/jasminewd2": "~2.0.2",
    "@types/method-override": "0.0.29",
    "@types/morgan": "^1.7.32",
    "@types/node": "^8.0.47",
    "body-parser": "^1.17.2",
    "bootstrap": "^4.0.0",
    "codelyzer": "~4.0.0",
    "core-js": "^2.5.3",
    "express": "^4.16.3",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~2.0.5",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "method-override": "^2.3.9",
    "morgan": "^1.9.0",
    "protractor": "~5.1.2",
    "rimraf": "^2.6.1",
    "rxjs": "^6.2.2",
    "sw-precache": "^5.2.0",
    "ts-loader": "^3.5.0",
    "ts-node": "^7.0.0",
    "tslint": "~5.11.0",
    "typescript": "^2.9.2",
    "zone.js": "^0.8.26",
    "rxjs-compat": "^6.0.0-rc.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "nodemon": "^1.18.3"
  }
}

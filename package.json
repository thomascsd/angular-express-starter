{
  "name": "angular-express-starter",
  "version": "0.4.0",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/server/index.js",
    "dev:server": "nodemon -e ts -w ./server -x npm run server",
    "dev:client": "ng serve --proxy-config proxy.conf.json -o",
    "server": "ts-node --project src/server/tsconfig.json ./src/server/index.ts",
    "deploy": "git push origin master && git push heroku master",
    "build": "npm run build:server && npm run build:client",
    "build:client": "ng build --prod",
    "build:server": "tsc --project src/server",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.1",
    "@angular/cdk": "^9.0.0",
    "@angular/cli": "9.0.2",
    "@angular/common": "9.0.1",
    "@angular/compiler": "9.0.1",
    "@angular/compiler-cli": "9.0.1",
    "@angular/core": "9.0.1",
    "@angular/forms": "9.0.1",
    "@angular/language-service": "9.0.1",
    "@angular/material": "^9.0.0",
    "@angular/platform-browser": "9.0.1",
    "@angular/platform-browser-dynamic": "9.0.1",
    "@angular/platform-server": "9.0.1",
    "@angular/router": "9.0.1",
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "^3.3.13",
    "@types/jasminewd2": "~2.0.6",
    "@types/method-override": "0.0.31",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.11.1",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "codelyzer": "^5.1.2",
    "core-js": "^3.1.4",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "method-override": "^3.0.0",
    "morgan": "^1.9.1",
    "protractor": "~5.4.2",
    "rimraf": "^3.0.2",
    "routing-controllers": "^0.8.1-alpha.2",
    "rxjs": "^6.5.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tslib": "^1.10.0",
    "tslint": "~6.0.0",
    "typedi": "^0.8.0",
    "typescript": "^3.7.5",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.2",
    "nodemon": "^2.0.2"
  }
}

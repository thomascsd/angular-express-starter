{
  "name": "angular-express-starter",
  "version": "0.4.0",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/server/index.js",
    "dev:server": "nodemon -e ts -w ./server -x npm run server",
    "dev:client": "ng serve --proxy-config proxy.conf.json -o",
    "server": "ts-node --project src/server/tsconfig.json ./src/server/index.ts",
    "deploy": "git push origin master && git push heroku master",
    "build": "npm run build:server && npm run build:client",
    "build:client": "ng build --prod",
    "build:server": "tsc --project src/server",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.0.8",
    "@angular/cdk": "^9.1.2",
    "@angular/cli": "10.0.5",
    "@angular/common": "10.0.8",
    "@angular/compiler": "10.0.8",
    "@angular/compiler-cli": "10.0.8",
    "@angular/core": "10.0.8",
    "@angular/forms": "10.0.8",
    "@angular/language-service": "10.0.8",
    "@angular/material": "^9.1.2",
    "@angular/platform-browser": "10.0.8",
    "@angular/platform-browser-dynamic": "10.0.8",
    "@angular/platform-server": "10.0.8",
    "@angular/router": "10.0.8",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.7",
    "@types/jasmine": "^3.5.12",
    "@types/jasminewd2": "~2.0.8",
    "@types/method-override": "0.0.31",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.27",
    "airtable-node": "^0.1.20",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.2",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "codelyzer": "^6.0.0",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "ngx-dynamic-form-builder": "^1.5.4",
    "ngx-loading": "^8.0.0",
    "ngx-take-until-destroy": "^5.4.0",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "routing-controllers": "0.9.0-alpha.6",
    "rxjs": "^6.6.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tslib": "^2.0.0",
    "tslint": "^6.1.3",
    "typedi": "^0.8.0",
    "typescript": "^3.9.7",
    "zone.js": "~0.10.3"
  },
